<h1>New Match</h1>

<% form_for(@match) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :admin_id %><br />
    <%= f.text_field :admin_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :github_user %><br />
    <%= f.text_field :github_user %>
  </p>
  <p>
    <%= f.label :github_project %><br />
    <%= f.text_field :github_project %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', matches_path %>
